{
	"name": "@commercetools/frontend-sdk",
	"version": "1.6.0",
	"license": "UNLICENSED",
	"main": "lib/index.js",
	"scripts": {
		"dev": "tsup src/index.ts -d lib --dts --watch",
		"build": "tsup src -d lib --format esm,cjs --dts",
		"build-bak": "esbuild --outdir=lib --bundle --platform=node --format=cjs src/index.ts && yarn build:types",
		"release": "tsup src -d lib --format esm,cjs --dts",
		"release-bak": "esbuild --outdir=lib --bundle --platform=node --format=cjs --minify src/index.ts && yarn release:types",
		"prepublishOnly": "rm -rf lib && yarn install && yarn release",
		"prettier:fix": "prettier --write .",
		"test": "vitest"
	},
	"dependencies": {
		"@frontastic/extension-types": "^0.0.11",
		"cookies": "^0.8.0",
		"js-cookie": "3.0.1"
	},
	"devDependencies": {
		"@types/cookies": "^0.7.7",
		"@types/core-js": "2.5.5",
		"@types/js-cookie": "3.0.2",
		"@types/node": "^18.14.0",
		"prettier": "2.7.1",
		"tsup": "^6.7.0",
		"typescript": "^4.9.3",
		"vitest": "^0.29.2"
	}
}
